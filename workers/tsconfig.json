                "moduleResolution": "bundler",
                "lib": [
                        "esnext"
                ],
                "types": [
                        "@cloudflare/workers-types"
                ],
                "noEmit": true,
                "strict": true,
                "skipLibCheck": true,
                "esModuleInterop": true
        },
        "include": [
                "src/**/*.ts"
        ]
}